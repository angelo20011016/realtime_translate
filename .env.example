# Environment variables for the Real-time Translator

# Your Google Gemini API Key
GEMINI_API_KEY="YOUR_GEMINI_API_KEY"

# Your Azure Cognitive Services Speech Key and Region
AZURE_SPEECH_KEY="YOUR_AZURE_SPEECH_KEY"
AZURE_SPEECH_REGION="YOUR_AZURE_SPEECH_REGION"

# A secret key for Flask sessions (optional, but good practice)
SECRET_KEY="a-super-secret-key-that-you-should-change"
  
# Google SSO  
GOOGLE_CLIENT_ID=  
GOOGLE_CLIENT_SECRET= 
REDIRECT_URI=http://localhost:5000/authorize 
